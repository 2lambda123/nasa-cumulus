{
  "name": "@cumulus/update-dla-format",
  "version": "18.2.0",
  "author": "Cumulus Authors",
  "description": "update old DLA records to new format",
  "license": "Apache-2.0",
  "private": true,
  "homepage": "https://github.com/nasa/cumulus/tree/master/lambdas/update-dla-format#readme",
  "repository": {
