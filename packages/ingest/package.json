{
  "name": "@cumulus/ingest",
  "version": "1.23.2",
  "description": "Ingest utilities",
  "engines": {
    "node": ">=10.16.3"
  },
  "scripts": {
    "test": "../../node_modules/.bin/ava",
    "test-coverage": "../../node_modules/.bin/nyc npm test"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "!test/fixtures/**/*"
    ],
    "timeout": "15m"
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/ingest",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/aws-client": "1.23.2",
    "@cumulus/common": "1.23.2",
    "@cumulus/errors": "1.23.2",
    "@cumulus/message": "1.23.2",
    "@cumulus/sftp-client": "1.23.2",
    "aws-sdk": "^2.585.0",
    "cksum": "^1.3.0",
    "delay": "^4.3.0",
    "encodeurl": "^1.0.2",
    "fs-extra": "^5.0.0",
    "got": "^9.2.1",
    "is-ip": "^2.0.0",
    "is-valid-hostname": "^0.0.1",
    "jsftp": "https://github.com/jkovarik/jsftp.git#add_288",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.22",
    "moment": "2.24.0",
    "simplecrawler": "^1.1.4",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@cumulus/checksum": "1.23.2",
    "@cumulus/cmrjs": "1.23.2",
    "@cumulus/test-data": "1.23.2"
  }
}
