{
  "name": "@cumulus/tea-map-cache",
  "version": "1.22.1",
  "description": "Tea Bucket Map Cache Writer",
  "main": "index.js",
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "package": "rm -Rf ./dist/ && mkdir dist && zip -r -q ./dist/lambda.zip *.js node_modules",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "fail-fast": true,
    "verbose": true,
    "timeout": "2m"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "aws-sdk": "^2.585.0",
    "@cumulus/logger": "1.24.0",
    "got": "^9.6.0",
    "p-retry": "^4.2.0"
  },
  "private": true
}
