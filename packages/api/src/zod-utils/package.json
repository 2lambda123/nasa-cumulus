{
  "name": "@cumulus/zod-utils",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=14.19.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.map"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prepare": "npm run tsc",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "test": "../../node_modules/.bin/ava",
    "tsc:listEmittedFiles": "../../node_modules/.bin/tsc --listEmittedFiles",
    "tsc": "../../node_modules/.bin/tsc",
    "watch": "../../node_modules/.bin/tsc -w"
  },
  "dependencies": {
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "typescript": "^4.9.5"
  }
}
