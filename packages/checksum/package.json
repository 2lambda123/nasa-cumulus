{
  "name": "@cumulus/checksum",
  "version": "1.22.1",
  "description": "Cumulus checksum utilities",
  "engines": {
    "node": ">=10.16.3"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*.js",
    "./dist/**/*.d.ts"
  ],
  "scripts": {
    "test": "ava",
    "test-coverage": "nyc ava",
    "debug": "NODE_ENV=test node --inspect-brk node_modules/ava/profile.js --serial tests/*.js",
    "tsc": "tsc",
    "prepare": "npm run tsc"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "cksum": "^1.3.0"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "nyc": "^14.0.0",
    "typescript": "^3.8.3"
  }
}
