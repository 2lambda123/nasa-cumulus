{
  "name": "@cumulus/post-to-cmr",
  "version": "1.0.0-beta.16",
  "description": "Post a given granule to CMR",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cumulus-nasa/cumulus"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "TEST=true ava tests/*.js --serial --no-color",
    "build": "webpack --progress",
    "watch": "webpack --progress -w",
    "postinstall": "npm run build"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/cmrjs": "^1.0.0-beta.16",
    "@cumulus/common": "^1.0.0-beta.16",
    "@cumulus/ingest": "^1.0.0-beta.16",
    "@cumulus/test-data": "^1.0.0-beta.16",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.24.1",
    "json-loader": "^0.5.7",
    "lodash.get": "^4.4.2",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "@ava/babel-preset-transform-test-files": "^3.0.0",
    "ava": "^0.21.0",
    "proxyquire": "^1.8.0",
    "sinon": "^2.0.0-pre.5"
  }
}
