{
  "title": "MoveGranulesOutput",
  "description": "Describes the output produced by the move-granules task",
  "type": "object",
  "properties": {
    "granules": {
      "type": "array",
      "description": "Array of all granules",
      "items": {
        "type": "object",
        "required": ["granuleId", "files"],
        "properties": {
          "granuleId": {
            "type": "string"
          },
          "files": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["name"],
              "properties": {
                "name": {
                  "type": "string"
                },
                "filename": {
                  "type": "string"
                },
                "bucket": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "inputFiles": {
      "type": "array",
      "description": "list of filenames from input"
    }
  }
}