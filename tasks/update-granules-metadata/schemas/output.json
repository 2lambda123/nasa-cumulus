{
  "title": "UpdateGranulesMetadataOutput",
  "description": "Describes the output produced by the update-granules-metadata task",
  "type": "object",
  "properties": {
    "granules": {
      "type": "array",
      "description": "Array of all granules",
      "items": {
        "type": "object",
        "required": ["granuleId", "files"],
        "properties": {
          "granuleId": {
            "type": "string"
          },
          "files": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["name"],
              "properties": {
                "name": {
                  "type": "string"
                },
                "filename": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "bucket": {
                  "type": "string"
                },
                "etag": {
                  "description": "Entity tag for the current version of the file",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
