{
  "name": "gibs-ops-api",
  "version": "0.1.0",
  "description": "Implements the API backing the GIBS Ops Dashboard",
  "main": "lambda.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "npm run build && node dist/local.js",
    "start-debug": "npm run build && node --inspect dist/local.js",
    "start-debug-brk": "npm run build && node --inspect-brk dist/local.js",
    "start-watch": "bin/parallel.sh 'webpack --config webpack.config.js  -w' 'nodemon dist/local.js -w dist'"
  },
  "dependencies": {
    "aws-serverless-express": "^2.2.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "html-loader": "^0.4.5",
    "markdown-loader": "^2.0.0",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nodemon": "^1.11.0",
    "webpack": "^2.4.1"
  }
}
